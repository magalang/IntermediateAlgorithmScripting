
function truthCheck(collection, pre) {
  // Is everyone being true?
  var stat = true;
  collection.forEach(function(obj){
    if (obj[pre]){
    } else {
      stat = false;
    }
  });
  return stat;                    
}

truthCheck([{"user": "Tinky-Winky", "sex": "male"}, {"user": "Dipsy", "sex": "male"}, {"user": "Laa-Laa", "sex": "female"}, {"user": "Po", "sex": "female"}], "sex");
